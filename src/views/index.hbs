<div class="jumbotron mt-4">
    <h1 class="display-4">Proyecto 2 </h1>
</div>
<div class="container mt-5">
    <div class="row">
        <div class="col-md-12 offset-md-1">
            <hr>
            <h3>Datos de Mongo</h3>
            <div id="ventana-stats" class="card">
                <table class="table table-striped table-dark">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Departamento</th>
                            <th>Edad</th>
                            <th>Forma de contagio</th>
                            <th>Estado</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each datos}}
                            <tr>
                                <td>{{Nombre}}</td>
                                <td>{{Departamento}}</td>
                                <td>{{Edad}}</td>
                                <td>{{Forma_contagio}}</td>
                                <td>{{Estado}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
            <hr>
            <h3>Datos de Redis</h3>
            <h4>ultimo dato registrado</h4>
            <div id="ventana-stats" class="card">
                <table class="table table-striped table-dark">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Departamento</th>
                            <th>Edad</th>
                            <th>Forma de contagio</th>
                            <th>Estado</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>{{datosFormRedis.Nombre}}</td>
                            <td>{{datosFormRedis.Departamento}}</td>
                            <td>{{datosFormRedis.Edad}}</td>
                            <td>{{datosFormRedis.Forma_contagio}}</td>
                            <td>{{datosFormRedis.Estado}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
