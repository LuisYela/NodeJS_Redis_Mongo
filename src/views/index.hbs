<div class="jumbotron mt-4">
    <h1 class="display-4">Proyecto 2 </h1>
</div>
<div class="container mt-5">
    <div class="row">
        <div class="col-md-12 offset-md-1">
            <hr>
            <h3>Datos de Mongo</h3>
            <div id="ventana-stats" class="card">
                <table class="table table-striped table-dark">
                    <thead>
                        <tr>
                            <th>Departamento</th>
                            <th>DPI</th>
                            <th>Edad</th>
                            <th>Fecha de registro</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            {{#each datos}}
                                <td>{{departamento}}</td>
                                <td>{{dpi}}</td>
                                <td>{{edad}}</td>
                                <td>{{fecha}}</td>
                            {{/each}}
                        </tr>
                    </tbody>
                </table>
            </div>
            <hr>
            <h3>Datos de Redis</h3>
            <div id="ventana-stats" class="card">
                <table class="table table-striped table-dark">
                    <thead>
                        <tr>
                            <th>Departamento</th>
                            <th>DPI</th>
                            <th>Edad</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>{{datosFormRedis.departamento}}</td>
                            <td>{{datosFormRedis.dpi}}</td>
                            <td>{{datosFormRedis.edad}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
